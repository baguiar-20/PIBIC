#include<stdio.h>
#include <stdlib.h>
#include <math.h>


struct pnt {
float a1,a2,b1,b2;
};

int main() {

float d;
struct pnt dist;


scanf ("%f",&dist.a1);

scanf ("%f",&dist.a2);

scanf ("%f",&dist.b1);

scanf ("%f",&dist.b2);


d = sqrt( pow (dist.a2 - dist.a1, 2) + pow (dist.b2 - dist.b1, 2));;

printf("%1.f",d);
}
