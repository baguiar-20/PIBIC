#include <stdio.h>

int main() {
	int linha, coluna;
	int matriz[7][7] = {{0,2,11,6,15,11,1},
						{2,0,7,12,4,2,15},
						{11,7,0,11,8,3,13},
						{6,12,11,0,10,2,1},
						{15,4,8,10,0,5,13},
						{11,2,3,2,5,0,14},
						{1,15,3,1,13,14,0}};

	scanf("%d %d", &linha, &coluna);
	while(linha!=coluna) {
		printf("%d\n",matriz[linha][coluna]);
		scanf("%d %d", &linha, &coluna);
	}
	
	return 0; 
}
