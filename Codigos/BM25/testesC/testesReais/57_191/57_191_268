#include <stdio.h>
#include <stdlib.h>

int main(){
	int n,i,div,numero,cont;
	cont=0;
	div=0;
	printf("\n");
	scanf("%d",&n);
	numero = n+1;
    
	while( cont<1){
		div=0;
	for(i=2;i<=numero;i++){
		if(numero%i==0){
			div++;
		}		
      }
      if(div==1){
			printf("%d",numero);
			cont++;
		}
	numero++;		
   }
return 0;
}
