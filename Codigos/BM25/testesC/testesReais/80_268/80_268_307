#include<stdio.h>
#include <stdio.h>
#include <stdlib.h>

int main() {

int i,j;
char A[50],B[50],C[50];

gets(A);
gets(C);
 
for(i=0;A[i];i++);
i = i-1; 
for(j=0,i;i>=0;j++){ 

B[j]=A[i];
i--;

}

B[j]='\0';

                     if(B[i]==C[i]){
                                    printf("SIM");
                                    }
                                    else{
                                         printf("NAO");
                                         }
                                    
}
