#include <stdio.h>
#include <stdlib.h>

int main() {

int i,j;
char st1[100],st2[100],st3[100];


gets(st1);
gets(st3);
for(i=0;st1[i];i++);
i = i-1;
for(j=0,i;i>=0;j++){

st2[j]=st1[i];
i--;

}

st2[j]='\0';

if(st2==st3)
{
    printf("NAO");
}
else
{
    printf("SIM");
}

}
