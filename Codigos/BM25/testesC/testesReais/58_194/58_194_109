#include "stdio.h"
int i;

int Primo(int num)
{
	int div;
	div = 0;
	
	for (i = 1;i <= num;i++)
	{
		if(num % i == 0)
		{
			div++;
		}
	}
	
	if(div == 2)
	{
		return 0;
	}
	else
	{
		return 1;
	}
}

int main()
{
	int n1, n2, mmc;
	
	scanf("%d%d", &n1, &n2);
	mmc = 1;
	
	for(i = 1;i = i;i++)
	{
		while ((Primo(i) == 0) && (n1 % i == 0) && (n2 % i == 0))
		{
			mmc = mmc * i;
			
			if(n1 % i == 0)
			{
				n1 = n1 / i;
			}
			if(n1 % i == 0)
			{
				n2 = n2 / i;
			}
		}	
	}
	
	printf("%d\n", mmc);
	
	
	
	
	
	
	
	
	return 0;
}
