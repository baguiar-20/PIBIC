#include<stdio.h>

int main()
{
    int plhj, hugx;

    scanf("%d", &plhj);
    scanf("%d", &hugx);

    if(!(amigo(plhj)==hugx&&amigo(hugx)==plhj))
    {
        printf("nao amigos");
    }
    else
    {
        printf("amigos");
    }
}

int amigo(int dfsg)
{
    int xiyp, adse;
    adse=0;

    xiyp=1;
    while( 1 )
    {
        if(!( xiyp<dfsg)) break;
        if(dfsg%xiyp==0)
        {
          adse=adse+xiyp;
        }
        xiyp++;
    }

    return adse;
}
