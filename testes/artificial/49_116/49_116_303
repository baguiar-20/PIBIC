#include <stdio.h>
//#include <conio.h>

void pause(){
     
    // getch();
     
     }
     
int somaDivisores(int n){

    int i,soma;
    
    soma=0;
    
    for(i=1;i<n;i++){
                        
                        
                        if(n%i==0){
                               soma+=i;  
                        }                 
                        
    }
    
    return soma;

}

int main(void){
    
    int num,num2;
    
    scanf("%d %d",&num,&num2);

    if(somaDivisores(num)==num2 && somaDivisores(num2)==num){
    
                       printf("amigos");                         
    
    }else{
          
                       printf("nao amigos");
    
    }
    
    
    pause();
    return 0;
    
}
