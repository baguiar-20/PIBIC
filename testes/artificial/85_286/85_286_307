#include<stdio.h>
#include<stdlib.h>
#include<malloc.h>

int busca( int *vet, int tam, int pos){
   int i;
   for(i=0;i<tam;i++){
	if(vet[i] == pos){
	return i;
	}
	}

	return -1;
}

int main(){

        int j,val,n,resultado;
	int *vetor;

vetor = (int*) malloc (n*sizeof(int));
if (vetor == NULL){

printf("Sem memÃ³ria \n");

}

	scanf("%d",&n);

while(n>0){

for(j=0;j<n;j++){
   	scanf("%d",&vetor[j]);
}

	scanf("%d",&val);

	resultado = busca(vetor,n,val);
       		if(resultado == -1){ 
		   printf("NAO \n");
       		}else{
                   printf("SIM \n");
                }

        scanf("%d",&n);

}
free(vetor);
}
