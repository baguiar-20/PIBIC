#include <stdio.h>

void main()
{
    int fatorial, n, f;
    
    scanf("%d", &n);
    fatorial = 1;
    
    for (f = n; f > 0; f--)
    {
        fatorial = fatorial * f;
    }
    
    printf("%d", fatorial);
    
}

