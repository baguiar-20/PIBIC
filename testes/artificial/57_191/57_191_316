#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(int argc, const char * argv[])
{
    int bbhj;
    scanf("%d",&bbhj);
    int pytr;

	    pytr=2;
	    while (1){
	    	if (!(pytr < bbhj)) break;
	    	if (bbhj % pytr == 0){
	    		bbhj++;
	    		pytr = 2;
	    	}
	    	pytr++;

	    }
		printf("%d",bbhj);
	

    return 0;
}
