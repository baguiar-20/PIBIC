#include <stdio.h>
#include <string.h>
#include <ctype.h>
#define TAM 40

int main() {

    int conjunto[TAM];
    int i = 0;
    int j;
    int num_atual;

    printf("Numeros: ");

    while (scanf("%d", &num_atual) == 1) {
          conjunto[i] = num_atual;
          i++;
    }
    printf("Inverso: ");    
    for (j=(i-1); j>=0; j--) {
        printf("%d ", conjunto[j]);
    }
    printf("\n");
    return 0;
}
