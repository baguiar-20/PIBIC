#include <stdio.h>
#include <string.h>

int main(){

     int i=0,j=0,aux;
     int vetor[100],num;

     printf("Numeros: ");
     while (scanf("%d", &num) == 1)
     {
       vetor[i] = num;
       i++;
     }

     printf("Inverso: ");
     for (j=i-1; j!=-1;j--)
     {
         printf("%d ",vetor[j]);
     }

    printf("\n");

}
