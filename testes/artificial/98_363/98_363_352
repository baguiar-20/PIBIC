#include <stdio.h>


int main () {

   int i = 0, j = 0, numero[100],num;
 
   printf("Numeros: ");
   while (scanf("%d", &num) == 1) {
      numero[i] = num;
      i++;
      j++;
   }
 
	

   printf("Inverso: ");	
   for(i = j-1; i >= 0 ; i--) {  
	printf("%d ",numero[i]);
   }

   printf("\n");

getchar();
return 0;
}
