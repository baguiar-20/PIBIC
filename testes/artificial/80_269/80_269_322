#include<stdio.h>
#include<string.h>

int main(){

  char bcdv[100];
  char xvdf ;
  int cgnd,mkbv=0;

     gets(bcdv);
     scanf("%c", &xvdf);
       
 cgnd=0;
 while(1){
    if(!(cgnd<strlen(bcdv))) break;
    if(bcdv[cgnd]==xvdf)
          mkbv++;  
    cgnd++;
    }

        printf("%d",mkbv);




}

