#include <stdio.h>
#include <stdlib.h>

int main(){
int v[15];
int x,i,aux=-1;
for(i=0;i<15;i++){
                  scanf("%d",&v[i]);
                  }
                  scanf("%d",&x);
                  for(i=0;i<15;i++){
                                    if(v[i]==x)
                                    aux=i;
                                    }
                                     if(aux==-1){
                                                 printf("NAO");
                                                 }
                                                 else{
                                                 printf("%d",aux+1);
                                                 }
return 0;
}
