#include <stdio.h>
#include <stdlib.h>

int main()
{
  int n,m;
  scanf("%d",&n);
   scanf("%d",&m);
  int soma = 0;
  int soma2=0;
  int i;
  
  
  

  for(i=1;i<=n-1;i++){
					if(n % i == 0){
						 soma = soma + i;
						 }
                      }
                      

  for(i=1;i<=m-1;i++){
					if(m % i == 0){
						 soma2 = soma2 + i;
						 }
                      }                                          
                      
  
  if(soma==m) {
              printf("amigos");
              }
              else{
              printf("nao amigos");
              }
  
  
}
