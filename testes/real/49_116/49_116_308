#include<stdio.h>
#include<math.h>

int main(){

	int num1, num2, i, soma, soma2;

	scanf("%d", &num1);
	scanf("%d", &num2);

	soma=0;
	soma2=0;

	for(i=1; i<num1; i++)
	 {
	   if(num1 % i == 0)
 	    {
	      soma=soma+i;
	    }
	 }	      

	for(i=1; i<num2; i++)
	 {
	   if(num2 % i == 0)
 	    {
	      soma2=soma2+i;
	    }
	 }
	
	if(soma == num2 && soma2 == num1)
	 {
	   printf("amigos");	
	 }else
	 {
	   printf("nÃ£o amigos");
	 }
}
