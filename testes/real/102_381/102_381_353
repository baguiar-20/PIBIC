#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main () {


	float n1, n2, n3, n4, media = 0;
	int i = 0, a;

	while ((scanf("%f %f %f %f", &n1, &n2, &n3, &n4) == 4)&&(n1!='F')) {
    	
    	i++;
	
	if (n1 + n2 + n3 + n4 >= media) {
		media = n1 + n2 + n3 + n4;
		a = i;}
	else
		media;
	}
	
	
	printf ("%.3f do aluno %d\n", media/4, a);

	
	
	
	return 0;


}
