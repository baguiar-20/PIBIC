#include <stdio.h>
#include <stdlib.h>

int primo(int x){
if(((x%2==0)||(x%3==0))&&((x!=2)&&(x!=3))){
                                       return 0;
                                       }
                                       else{
                                       return 1;
                                       }
}
int main(){
int n;

do{
    scanf("%d",&n);
}while((n<1)||(n>1000));
if(primo(n)==1){
                printf("SIM");
                }
                else if(primo(n)==0){
                     printf("NAO");
                     }
return 0;
}
