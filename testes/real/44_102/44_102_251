#include <stdio.h>

int main (void)
{
	int i,n,cont,multiplica;
	
	
	scanf ("%d",&n );
	cont = 0;
	multiplica=n;
	if ( (n==0)  || (n==1) ) 
	{
		printf("1");
	}	
	else if ( n<0) 
	{
		printf("Incorreto");
	}
	else
	{
	for (i=0;i<n-1;i++)
	{
		cont = cont+1;
		multiplica = (multiplica) * (n-cont);
		
	}
		printf("%d",multiplica);
	}	
		return 0;
	
}
	
