#include<stdio.h>
#include<string.h>
#define max 30

int main (void)
{
    
    int nota[max];
    int i,a,b,c,d,e,f,g,h,k,j,u;
    a=b=c=d=e=f=g=h=u=j=k= 0;
        
    for (i=0;i<max;i++)
    {
        scanf("%d",& nota[i]);
    }
    
    for(i=0;i<max;i++)
    {
           if(nota[i]==1)
                a++;
           if(nota[i]==2)
                b++;
           if(nota[i]==3)
                c++;
           if(nota[i]==4)
                d++;
           if(nota[i]==5)
                e++;
           if(nota[i]==6)
                f++;
           if(nota[i]==7)
                g++;
           if(nota[i]==8)
                h++;
           if(nota[i]==9)
                u++;
           if(nota[i]==10)
                j++;
           if(nota[i]==0)
                k++;
    }   
    
        printf("0 ");        
        for(i = 0; i < k ; i++)
        {
              printf(" *");
        }
           
        printf("\n1 ");        
        for(i = 0; i < a ; i++)
        {
              printf(" *");
        }
        
        printf("\n2 ");        
        for(i = 0; i < b ; i++)
        {
              printf(" *");
        }
        printf("\n3 ");   
        for(i = 0; i < c ; i++)
        {
              printf(" *");
        }
        printf("\n4 ");   
        for(i = 0; i < d ; i++)
        {
              printf(" *");
        }
        printf("\n5 ");   
        for(i = 0; i < e ; i++)
        {
              printf(" *");
        }        
        printf("\n6 ");   
        for(i = 0; i < f ; i++)
        {
              printf(" *");
        }
        printf("\n7 ");   
        for(i = 0; i < g ; i++)
        {
              printf(" *");
        }
        printf("\n8 ");   
        for(i = 0; i < h ; i++)
        {
              printf(" *");
        }
        printf("\n9 ");   
        for(i = 0; i < u ; i++)
        {
              printf(" *");
        }
        printf("\n10 ");   
        for(i = 0; i < j ; i++)
        {
              printf(" *");
        }
}
