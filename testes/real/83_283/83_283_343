#include <stdio.h>
#include<stdlib.h> 

void main(){

	int n;
	int i;
	int dimen;
	int v[1000];
	int flag=1;

	printf("Enter the size:\n");
	scanf("%d",&dimen);

	if (dimen>1000 || dimen==0){
		dimen=1000;		
	}

	printf("Enter the %d numbers :\n",dimen);
	for (i=0; i < dimen; i++){
		scanf("%d",&v[i]);	
		}

	printf("Enter the number you want to search:\n");
	scanf("%d",&n);

	for (i=0; i < dimen; i++){
		if (n==v[i]){
			flag=0;
		}
		}

	if (flag==0){
		printf("SIM"); 		
	
		}else{
		printf("NAO");
		}



}
