#include <stdio.h>
#include <stdlib.h>

int main() {

int i,j;
char a[100],b[100],c[100];


gets(a);
gets(c);
 
for(i=0;a[i];i++);
i = i-1; 
for(j=0,i;i>=0;j++){ 

b[j]=a[i];
i--;

}

b[j]='\0';




                     if(b[i]==c[i]){
                                    printf("SIM");
                                    }
                                    else{
                                         printf("NAO");
                                         }
                                    




}
