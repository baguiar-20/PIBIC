#include <stdio.h>
int main(){
	int tabela [7] [7]={{ 0, 2,11, 6,15,11, 1},
						{ 2, 0, 7,12, 4, 2,15},
						{11, 7, 0,11, 8, 3,13},
						{ 6,12,11, 0,10, 2, 1},
						{15, 4, 8,10, 0, 5,13},
						{11, 2, 3, 2, 5, 0,14},
						{ 1,15,13, 1,13,14, 0}};
	int c1=1,c2=0;
	while(c1!=c2){
	scanf("%d %d", &c1,&c2);
	if(c1!=c2 && (c1<7 && c1>=0))
	printf("%d\n",tabela[c1][c2]);
	else
	c1=c2;
	}
	return 0;
}
