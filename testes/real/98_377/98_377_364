#include <stdio.h> 

int main(){
  int x, y;
  int vetor[100][100];
  
  vetor[0][0]= 1;
  vetor[0][1]= 2;
  vetor[0][2]= 11;
  vetor[0][3]= 6;
  vetor[0][4]= 15;
  vetor[0][5]= 11;
  vetor[0][6]= 1;
  
  vetor[1][0]= 2;
  vetor[1][1]= 1;
  vetor[1][2]= 7;
  vetor[1][3]= 12;
  vetor[1][4]= 4;
  vetor[1][5]= 2;
  vetor[1][6]= 15;
  
  vetor[2][0]= 11;
  vetor[2][1]= 7;
  vetor[2][2]= 1;
  vetor[2][3]= 11;
  vetor[2][4]= 8;
  vetor[2][5]= 3;
  vetor[2][6]= 13;
  
  vetor[3][0]= 6;
  vetor[3][1]= 12;
  vetor[3][2]= 11;
  vetor[3][3]= 1;
  vetor[3][4]= 10;
  vetor[3][5]= 2;
  vetor[3][6]= 1;
  
  vetor[4][0]= 15;
  vetor[4][1]= 4;
  vetor[4][2]= 8;
  vetor[4][3]= 10;
  vetor[4][4]= 1;
  vetor[4][5]= 5;
  vetor[4][6]= 13;
  
  vetor[5][0]= 11;
  vetor[5][1]= 2;
  vetor[5][2]= 3;
  vetor[5][3]= 2;
  vetor[5][4]= 5;
  vetor[5][5]= 1;
  vetor[5][6]= 14;
  
  vetor[6][0]= 1;
  vetor[6][1]= 15;
  vetor[6][2]= 13;
  vetor[6][3]= 1;
  vetor[6][4]= 13;
  vetor[6][5]= 14;
  vetor[6][6]= 1;
  
  scanf("%d %d", &x, &y);
  while(x!=y){
    if( (x>=0 && x<7)&&(y>=0 && y<7) ){
    printf("%d\n",vetor[x][y]);
    scanf("%d %d", &x, &y);
    }else{
      break;
    }
  }
}