#include<stdio.h>
int main(){
int n, m, i, j;
scanf("%i%i", &n,&m);
int A[n][m], B[n][m], C[n][m];
for(i=0;i<n;i++){
 for(j=0;j<m;j++){
   scanf("%i", &A[i][j]);}}
   
for(i=0;i<n;i++){
 for(j=0;j<m;j++){
   scanf("%i", &B[i][j]);
}}

for(i=0;i<n;i++){
 for(j=0;j<m;j++){
     C[i][j]=B[i][j]+A[i][j];}}
   
for(i=0;i<n;i++){
 for(j=0;j<m;j++){
    printf("%i ", C[i][j]);}printf("\n");}  
}
